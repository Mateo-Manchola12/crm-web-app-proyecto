---
import '@css/global.css'
interface Props {
    title: string
}

const { title } = Astro.props as Props
---

<!doctype html>
<html lang="es">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width" />
        <title>{title}</title>
    </head>
    <body>
        <slot />
    </body>
</html>
